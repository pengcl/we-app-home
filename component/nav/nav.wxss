.nav {
  width: 100%;
  overflow: hidden;
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
}

.nav-goback {
  text-align: left;
  position: absolute;
  bottom: 0;
  left: 13px;
  z-index: 66;
  letter-spacing: 2px;
  font-size: 30rpx;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  line-height: 46px;
  background-color: white;
}

.nav-title {
  width: 100%;
  height: 45px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  letter-spacing: 2px;
  font-size: 37rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #070707;
  line-height: 46px;
  background-color: white;
}

.nav .back {
  width: 22px;
  height: 22px;
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 10px 15px;
}

.bg-white {
  background-color: #32dfeb;
}

.bg-gray {
  background-color: #f7f7f7;
}

.bg-white {
  background-color: white;
}

.overflow {
  overflow: auto;
}

.hidden {
  overflow: hidden;
}
